{
  "name": "demo",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start:dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "npx vitest run",
    "lint": "npx eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "npx eslint \"src/**/*.{ts,js}\" --fix",
    "format": "npx prettier --w \"src/**/*.{ts,js}\" --ignore-unknown",
    "format:check": "npx prettier --c \"src/**/*.{ts,js}\" --ignore-unknown"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.6",
    "semantic-release-preconfigured-conventional-commits": "^1.1.152",
    "typedoc": "^0.28.12",
    "typedoc-github-theme": "^0.3.1"
  },
  "engines": {
    "node": "22.21"
  },
  "dependencies": {
    "globals": "^16.5.0"
  }
}
